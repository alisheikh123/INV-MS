@model IEnumerable<Inventory_Management_Systems.Models.tblAccountHead>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.accountHeadName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.account_Head_Code)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.accountHeadName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.account_Head_Code)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.accountHeadId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.accountHeadId">Details</a> |
